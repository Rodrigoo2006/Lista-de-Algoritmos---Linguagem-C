#include <stdio.h>

int main() {
    
    float prova1, prova2, prova3, media;
    
    prova1 = 2.0;
    prova2 = 3.0;
    prova3 = 5.0;
    
    printf("DIGITE A SUA PRIMEIRA NOTA (MAX 2):");
    scanf ("%f", &prova1);
    
    printf("DIGITE A SEGUNDA NOTA (MAX 3):");
    scanf ("%f", &prova2);
    
    printf("DIGITE A TERCEIRA NOTA (MAX 5):");
    scanf ("%f", &prova3);
    
    media = (prova1 + prova2 + prova3) /3;
    
    printf ("SUA PRIMEIRA NOTA: %.2f \n", prova1);
    printf ("SUA SEGUNDA NOTA: %.2f \n", prova2);
    printf ("SUA TERCEIRA NOTA: %.2f \n", prova3);
   
    printf ("SUA MEDIA: %.2f \n", media);
    
    if (media > 10) {
        printf ("VALOR IMPOSSIVEL");
    }
    
    return 0;
}
